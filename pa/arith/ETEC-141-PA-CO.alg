var
nome: caractere
salario: real
vendas: real
inicio
escreval("Vendedor: nome, salário fixo, total de vendas")
leia(nome, salario, vendas)
escreval("Total Salário (+15%): ", nome, ": ", salario+(vendas/100*15))
fimalgoritmo
